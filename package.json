{
  "name": "nest-demo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run build && npm run typeorm -- --dataSource=typeorm.config.ts migration:generate",
    "migration:run": "npm run typeorm -- --dataSource=typeorm.config.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=typeorm.config.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=typeorm.config.ts schema:drop",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/module/database/seeds/run-seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.4",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^6.3.0",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.4.4",
    "@types/passport-jwt": "^4.0.1",
    "argon2": "^0.31.2",
    "axios": "^1.6.7",
    "bull": "^4.15.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.4.0",
    "dotenv": "^16.4.1",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.0",
    "libphonenumber-js": "^1.10.54",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "nestjs-i18n": "^10.4.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "postgres-interval": "^4.0.2",
